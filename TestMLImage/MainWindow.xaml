<Window x:Class="TestMLImage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestMLImage"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="397" Margin="10,10,0,0" VerticalAlignment="Top" Width="513"/>
        <Button x:Name="buttonSelectImage" Content="画像選択" HorizontalAlignment="Left" Margin="540,10,0,0" VerticalAlignment="Top" Click="buttonSelectImage_Click"/>
        <Button x:Name="buttonAnalyzeImage" Content="解析" HorizontalAlignment="Left" Margin="597,10,0,0" VerticalAlignment="Top" Click="buttonAnalyzeImage_ClickAsync"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="540,61,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Width="232"/>
    </Grid>
</Window>
